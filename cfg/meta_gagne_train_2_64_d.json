{
    "experiment": {
        "type"            : "CEU",
        "runs"            : 1,
        "test_interval"   : 0,
        "max_evaluations" : 50,
        "function": {
            "type"      : [["F1.0", "F1.1", "F1.2"], ["F2.1", "F2.2"]],
            "dimension" : 2,
            "domain"    : [-5, 5]
        },
        "optimizer": {
            "type"                : "meta_model",
            "points_strategy"     : "random",
            "nb_points"           : 10,
            "hist_strategy"       : "gagne",
            "hist_init"           : "zero",
            "size_hf"             : 5,
            "best_point_strategy" : "lowest",
            "representation"      : {
                "type": "dnn",
                "path": "data/dnn/metamodel.h5"
            }
        },
        "trainer": {
            "type": "episodic_dnn",
            "dnn_path": "data/dnn/metamodel.h5",
            "data_path": "data/dataset/steps.npy",
            "batch_size": 2,
            "epochs": 64
        },
        "evaluator": {
            "type": "step_evaluator",
            "file_path": "data/output/meta_gagne_train_2_64"
        },
        "visualizer": {
            "type": null
        }
    }
}